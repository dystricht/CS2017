  <script type="text/javascript">
    document.observe("dom:loaded", function()
                     {
                       var link = $('submissionLink');
                       if(link)
                       {
                         link.removeClassName('expanded');
                         var fieldDiv = link.up('li').next('li');
                         fieldDiv.hide();
                       }
                     });

    </script>
  <script type="text/javascript">

    var sAttempt = 0;
    var arrAttemptFiles = new Array(sAttempt);
    var hasFileAttached = false;
    var submit_form = false;
    

    function checkDupeFile(action)
    {
      if (submit_form)
      {
          return false;
      }
      submit_form = true;
      var hasDuplicate = false;
      if ( $('newFile_table') )
      {
        var newFileTable = $('newFile_table').down("tbody");
        // fetch all rows from the table
        var tableRows=newFileTable.getElementsBySelector('tr');
        //all file names based on the link title from the table
        var allFileNames = newFileTable.getElementsBySelector('[name="newFile_linkTitle"]').pluck("value");
        //new attached file names based on span "filename"
        var newFileNames = newFileTable.getElementsBySelector("span.fileName").pluck("innerHTML");
        //cleaning up file names to remove the img tag from them
        newFileNames = cleanFileNames( newFileNames );
        hasFileAttached = allFileNames.length;
    var fileNameLengthMsg = '';
        for(var i=0;i<newFileNames.length;i++){
         if(newFileNames[i].length>125){
         fileNameLengthMsg = fileNameLengthMsg +"\n"+newFileNames[i];
         }
        }
        if(fileNameLengthMsg.length>0){
         alert('Please remove following files by clicking \'Do not attach\' and reattach them with shorter file names.  File names should be less than 125 in length.' + " \n" + fileNameLengthMsg);
         submit_form = false;
         return false;
         }


    //modified  for AS-153748 --if two fileNames are same & these two rows(files) are newly added ,
        //returns false & shows alert message to the user
        if( !checkUploadFileDup( newFileNames ) )
        {
            alert('A file with the same name already exists. Please delete one.')
            submit_form = false;
            return false;
        }
        for (var i=0; i<arrAttemptFiles.length; i++)
        {
          for (var j=0; j<newFileNames.length; j++ )
          {
            if ( arrAttemptFiles[i] == newFileNames[j] )
            {
              /* modified  for AS-153748 - get status of current row. if  fileNames are same and newly
               * uploading fileName is same as already  existed file(in server) and not marked for file
               * deletion, ask for confirmation to override
               */
              var status=tableRows[i].getElementsBySelector('td') [2].down('input[type="hidden"]');
              if(status !=undefined && status.disabled){
                hasDuplicate = true;
                break;
              }
            }
          }
        }
      }

      if ( hasDuplicate )
      {
        var conf = confirm('A file with the same name already exists. Replace the existing file?') ;
        if (conf && (action == 'save'))
          saveAssignment();
        else if (conf && (action == 'submit'))
          submitAssignment();
      }
      else
      {
        if (action == 'save')
          saveAssignment();
        else if (action == 'submit')
          submitAssignment();
      }

      return false;
    }

    function cancelSubmit()
    {
      document.forms.uploadAssignmentForm.dispatch.value= "cancel";
      document.forms.uploadAssignmentForm.submit();
    }

    function saveAssignment()
    {
      // Set textarea value to VTBE contents
      if ( typeof(finalizeEditors) == "function" )
      {
        finalizeEditors();
      }

      document.forms.uploadAssignmentForm.dispatch.value= "save";
      document.forms.uploadAssignmentForm.submit();
    }

    function submitAssignment()
    {
      valForm = validateForm();
      if (valForm == true)
      {
        var doSubmit = true;
        

        var submissionText = bbDomUtil.getElementById( 'studentSubmission.text' );
        var studentCommentsValue = $('student_commentstext').value;
        if ( doSubmit && !isPortfolioSelected() && ( !hasFileAttached && isEmptyWysiwyg(submissionText) && !studentCommentsValue ) )
        {
          doSubmit = confirm( page.bundle.getString("blankSubmissionWarning") );
        }

        if( doSubmit )
        {
          document.forms.uploadAssignmentForm.dispatch.value= "submit";
          document.forms.uploadAssignmentForm.submit();
        }
        else
        {
          submit_form = false;
          return false;
        }
      }
      else
      {
        submit_form = false;
        return false;
      }
    }

    function checkUploadFileDup( fileNames )
    {
        for( var i = 0; i<fileNames.length; i++ )
        {
            for( var j = i+1; j<fileNames.length; j++ )
            {
                if(fileNames[j] == fileNames[i])
                    return false;
            }
        }
        return true;
    }

    /*
     * Method to clean the files names plucked from the span.fileName.
     * From span.fileName we are plucking innerHTML due to that we are getting the img tag as well with it.
     *      e.g <img src="/images/ci/ng...t="File"> Document1.txt
     * When we compare the innerHTML with the actual file name it always fail. Below method is doing the
     * cleaning up for the values plucked. Method is removing the img tag as well as triming the value.
     * By doing that we are getting the actual file name with out any other tag.
     * Eventually which help us to have a proper equals match.
     */
    function cleanFileNames( fileNames )
    {
      var modifiedNames = [];
      for( var i = 0; i<fileNames.length; i++ )
      {
        var fileName = fileNames[i];
        var indexlt = fileName.indexOf('<');
        if( indexlt > -1 )
        {
          var indexgt = fileName.indexOf('>', indexlt);
          if( indexgt > 0 )
          {
            var modifiedName = fileName.substring( indexgt + 1 );
            modifiedNames[i] = modifiedName.replace(/^\s+|\s+$/g,"");
          }
          else
          {
            modifiedNames[i] = fileName;
          }
        }
        else
        {
          modifiedNames[i] = fileName;
        }
      }
      return modifiedNames;
    }

    function createPortfolio()
    {
      
        document.location='/webapps/portfolio/authorNewPortfolio?returnUrl=';
      
      // TODO: not sure that document.location is the best final answer.  refactor as required.
    }

    function isPortfolioSelected()
    {
      if( window.PortfolioPicker )
      {
        return PortfolioPicker.isPortfolioSelected('portfoliosId');
      }
      //since portfolios are not always required for assignments, if the js portfolio_picker does not exist, it means we don't need to care about having any portfolios so always return false
      return false;
    }

    </script>
  
