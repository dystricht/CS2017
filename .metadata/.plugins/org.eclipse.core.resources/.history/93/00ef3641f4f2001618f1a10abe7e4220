package com.dystricht.Lab02;

public class Iris extends Flower {
	
	private String color;
	
	/**
	 * Iris default constructor.
	 * Not edible.
	 * Default Iris color is Purple.
	 */
	public Iris(){
		super("Iris", false);
		this.color = "Purple";
	}
	
	public String getName(){
		return this.getFlowerName;
	}
}
